# -*- mode:conf -*-
[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = false
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Hack 12
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = off

# "block", "underline" or "ibeam"
cursor_shape = block

# $BROWSER is used by default

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
#modify_other_keys = false
#

[hints]
#font = Dejavu Sans Mono 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# if unset, will reverse foreground and background
#highlight = #2f2f2f

# vim: ft=dosini cms=#%s
[colors]

# special
foreground      = #b2b2b2
foreground_bold = #b2b2b2
cursor          = #b2b2b2
background      = #1f1f1f

# black
color0  = #1f1f1f
color8  = #b2b2b2

# red
color1  = #f2241f
color9  = #67b11d

# green
color2  = #67b11d
color10 = #b1951d

# yellow
color3  = #b1951d
color11 = 

# blue
color4  = #2f96dc
color12 = #2f96dc

# magenta
color5  = #bc6ec5
color13 = #bc6ec5

# cyan
color6  = #28def0
color14 = #28def0

# white
color7  = #b2b2b2
color15 = #b2b2b2
